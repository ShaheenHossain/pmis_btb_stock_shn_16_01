        </record>
        <record id="group_account_btb_stock_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="btb_stock.module_category_btb_user"/>
            <field name="implied_ids" eval="[(4, ref('btb_stock.group_account_btb_stock_user'))]"/>
        </record>
        <record id="group_account_btb_stock_admin" model="res.groups">
            <field name="name">Admin</field>
            <field name="category_id" ref="btb_stock.module_category_btb_user"/>
            <field name="implied_ids" eval="[(4, ref('btb_stock.group_account_btb_stock_manager'))]"/>
        </record>

    </data>
</odoo>