<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.module.category" id="module_category_btb_user">
            <field name="name">BTB Stock</field>
            <field name="sequence">4</field>
        </record>
        <record id="group_account_btb_stock_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="btb_stock.module_category_btb_user"/>
